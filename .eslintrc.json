{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
      },
    "rules": {
        "react/prefer-stateless-function": [0, {
            "ignorePureComponents": true
          }],
          "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
          "indent": ["error", 2],
          "react/prop-types": 0
        }
}